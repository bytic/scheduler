{
  "name": "bytic/scheduler",
  "description": "Scheduler library for bytic framework",
  "license": "MIT",
  "type": "library",
  "keywords": [],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/bytic/scheduler",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "bytic/cache": "^0.9 || ^1.0",
    "bytic/collections": "^1.0",
    "bytic/console": "^0.9 || ^1.0",
    "bytic/package-base": "^1.0",
    "bytic/utility": "^1.0",
    "opis/closure": "^4.3",
    "php-http/client-common": "^1.9 || ^2.0",
    "php-http/client-implementation": "^1.0",
    "php-http/discovery": "^1.6",
    "php-http/httplug": "^1.1 || ^2.0",
    "php-http/multipart-stream-builder": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0 || ^2.0",
    "symfony/process": "^6.0 || ^7.0",
    "symfony/http-client": "^6.2 || ^7.0"
  },
  "require-dev": {
    "bytic/config": "^1.0",
    "bytic/container": "^1.0 || ^2.0",
    "bytic/deployer-recipies": "^6.0 || ^7.0",
    "bytic/dotenv": "^4.0",
    "bytic/phpqatools": "^1.0",
    "nyholm/psr7": "^1.1",
    "php-http/mock-client": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Bytic\\Scheduler\\": "src"
    },
    "files": [
      "src/Utility/general.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bytic\\Scheduler\\Tests\\": "tests/src",
      "Bytic\\Scheduler\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "captainhook/plugin-composer": true,
      "ergebnis/composer-normalize": true,
      "phpstan/extension-installer": true,
      "ramsey/composer-repl": true,
      "bytic/phpqatools": true,
      "php-http/discovery": true
    }
  }
}
